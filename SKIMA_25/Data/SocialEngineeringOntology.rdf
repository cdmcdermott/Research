    <owl:NamedIndividual rdf:about="http://www.SEiCS.org/2020/Ontology#baiting">
        <rdf:type rdf:resource="http://www.SEiCS.org/2020/Ontology#Attack_Method"/>
        <rdf:type rdf:resource="http://www.SEiCS.org/2020/Ontology#Computer-based"/>
