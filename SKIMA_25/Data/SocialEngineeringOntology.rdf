    <owl:NamedIndividual rdf:about="http://www.SEiCS.org/2020/Ontology#attacker11">
        <rdf:type rdf:resource="http://www.SEiCS.org/2020/Ontology#Attacker"/>
        <craft_and_perform rdf:resource="http://www.SEiCS.org/2020/Ontology#craft_computer_viruses"/>
